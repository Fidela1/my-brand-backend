    "@babel/register": "^7.18.9",
    "@hapi/joi": "^17.1.1",
    "bcrypt": "^4.0.1",
    "bcryptjs": "^2.4.3",
    "better-npm-run": "^0.1.1",
    "body-parser": "^1.19.0",
